{
    "label": "Business policy for coc-truck.engine-mon",
    "description": "A simple engine monitoring service simulator demo",
    "service": {
      "name": "coc-truck.engine-mon",
      "org": "$HZN_ORG_ID",
      "arch": "amd64",
      "serviceVersions": [
        {
          "version": "1.0.0",
          "priority":{}
        }
      ]
    },
    "properties": [
        {
            "name": "coc-approved",
            "value": true
        },
        {
            "name": "security-level",
            "value": 3
        }
    ],
    "constraints": [
          "vehicle-type-code == TR09 OR vehicle-type-code == TR23"
    ],
    "userInput": [
      {
        "serviceOrgid": "$HZN_ORG_ID",
        "serviceUrl": "coc-truck.engine-mon",
        "serviceVersionRange": "[0.0.1,INFINITY)",
        "inputs": [   ]
      }
    ]
  }
  